<%= render "shared/headershow", back: "back.png", title: "LOGIN", image: "search.svg" %>
<%= render "shared/msg" %>
<%= form_for(resource, as: resource_name, html: {'data-turbo' => "false"}, url: session_path(resource_name)) do |f| %>
  <div class="field">
    <%= f.label :email %>
    <%= f.email_field :email, class: "texto", autofocus: true, autocomplete: "email" %>
  </div>

  <div class="field">
    <%= f.label :password %>
    <%= f.password_field :password, class: "texto", autocomplete: "current-password" %>
  </div>

  <div class="actions">
    <%= f.submit "Log in" %>
  </div>
<% end %>
<div class="shared-links">
  <%= render "devise/shared/links" %>
</div>


